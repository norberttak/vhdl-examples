# Makefile automatically generated by ghdl
# Version: GHDL 0.37 (Ubuntu 0.37+dfsg-1ubuntu1) [Dunoon edition] - mcode code generator

GHDL=/usr/bin/ghdl-mcode
GHDLFLAGS= -v --std=08 -fexplicit

.PHONY: all

# Default target : elaborate
all : init elab run

# Targets to analyze libraries
init:
	$(GHDL) -a $(GHDLFLAGS) ../common/clk_gen.vhdl
	$(GHDL) -a $(GHDLFLAGS) ../common/utest.vhdl
	$(GHDL) -a $(GHDLFLAGS) full-add.vhdl
	$(GHDL) -a $(GHDLFLAGS) full-add-tb.vhdl

# Elaborate target
elab:
	$(GHDL) -e $(GHDLFLAGS) full_add_tb

# Run target
run:
	$(GHDL) -r $(GHDLFLAGS) full_add_tb --wave=wave.ghw

clean:
	rm -f *.cf
	rm -f *.o
